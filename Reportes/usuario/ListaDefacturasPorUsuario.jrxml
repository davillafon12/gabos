	<parameter name="paEsSucursal" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT  user.Usuario_Nombre_Usuario usuario,
        CONCAT(user.Usuario_Nombre, " ", user.Usuario_Apellidos) nombre,
        fac.Factura_Consecutivo consecutivo,
        fac.Factura_Fecha_Hora fecha,
        fac.Factura_Monto_Total montoTotal,
        fac.Factura_Monto_IVA montoIVA,
        fac.Factura_Monto_Sin_IVA montoSinIVA
FROM    tb_01_usuario user inner join
        tb_07_factura fac on user.Usuario_Codigo = fac.Factura_Vendedor_Codigo and
                             user.TB_02_Sucursal_Codigo = fac.TB_02_Sucursal_Codigo inner join
        tb_03_cliente cli on fac.TB_03_Cliente_Cliente_Cedula = cli.Cliente_Cedula
where   user.TB_02_Sucursal_Codigo = $P{paSucursal} and
        fac.Factura_Estado = $P{paEstadoFactura} and
        cli.Cliente_EsSucursal = $P{paEsSucursal} and
        UNIX_TIMESTAMP(fac.Factura_Fecha_Hora) BETWEEN UNIX_TIMESTAMP($P{paFechaI}) AND UNIX_TIMESTAMP($P{paFechaF})]]>
	</queryString>
	<field name="usuario" class="java.lang.String"/>
	<field name="nombre" class="java.lang.String"/>
	<field name="consecutivo" class="java.lang.Integer"/>
	<field name="fecha" class="java.sql.Timestamp"/>
	<field name="montoTotal" class="java.lang.Double"/>
	<field name="montoIVA" class="java.lang.Double"/>
	<field name="montoSinIVA" class="java.lang.Double"/>
